use ether_be

db.createCollection("blocks");

db.blocks.createIndex({ blockHash: 1 }, { unique: true});
db.blocks.createIndex({ height: -1 });
db.blocks.createIndex({ timestamp: -1 });


db.createCollection("txs");

db.txs.createIndex({ txHash: 1 }, { unique: true});
db.txs.createIndex({ blockNumber: -1 });
db.txs.createIndex({ from: 1 });
db.txs.createIndex({ to: 1 });

